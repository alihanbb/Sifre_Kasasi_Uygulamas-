﻿@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

    @await Html.PartialAsync("/Views/AdminUserLayout/HeadPartial.cshtml")

<body>
    @await Html.PartialAsync("/Views/AdminUserLayout/PreloaderPartial.cshtml")

    <div id="main-wrapper">
        @await Html.PartialAsync("/Views/AdminUserLayout/NavheaderPartial.cshtml")

        @await Html.PartialAsync("/Views/AdminUserLayout/SidebarPartial.cshtml")
        <div class="content-body">

            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- #/ container -->
        </div>
        @await Html.PartialAsync("/Views/AdminUserLayout/FooterPartial.cshtml")
    </div>
    @await Html.PartialAsync("/Views/AdminUserLayout/ScriptPartial.cshtml")
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>

